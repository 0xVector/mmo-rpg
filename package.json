{
  "name": "mmo-rpg",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "bun-types": "latest",
    "typedoc": "^0.25.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "excalibur": "0.29.0-alpha.835"
  },
  "scripts": {
    "build": "bun build ./client/src/game.ts --outdir=client/out",
    "client": "bun run ./client/serve.ts",
    "dev": "bun run build && bun run client",
    "gen-docs": "bun run typedoc --entryPointStrategy expand ./client/src"
  }
}